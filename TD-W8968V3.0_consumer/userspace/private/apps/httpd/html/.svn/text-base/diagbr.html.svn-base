            <CENTER>
<script language="javascript">
<!-- hide
var diagPrev = '<%ejGetOther(diagInfo, diagPrev)%>';
var diagNext = '<%ejGetOther(diagInfo, diagNext)%>';
var sessionKey='<%ejGetOther(sessionKey)%>';
var GO_PREV = 0;
var GO_NEXT = 1;

function btnGo(where) {
   var loc = 'diag.cgi?diagCurr=';
    
   if ( where == GO_PREV )
      loc += diagPrev;
   else
      loc += diagNext;
      
   loc += '&sessionKey=' + sessionKey;
   var code = 'location="' + loc + '"';
   eval(code);
}

function btnTest(type) {
   var loc = 'diag.cgi?';

   loc += 'diagType=' + type;

   loc += '&sessionKey=' + sessionKey;
   var code = 'location="' + loc + '"';
   eval(code);
}

if ( diagPrev != '' )
   document.writeln("                  <input type='button' onClick='btnGo(GO_PREV)' value='Previous Connection'>");
if ( diagNext != '' )
   document.writeln("                  <input type='button' onClick='btnGo(GO_NEXT)' value='Next Connection'>");
// done hiding -->
</script>
                  <input type='button' onClick="btnTest('f5')" value='Test'>
                  <input type='button' onClick="btnTest('f4')" value='Test With OAM F4'>
