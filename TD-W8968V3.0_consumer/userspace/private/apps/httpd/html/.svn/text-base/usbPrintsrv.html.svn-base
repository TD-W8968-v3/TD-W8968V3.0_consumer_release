function doSetPrintSrv(status)
{
	var loc = 'usbPrintSrv.cmd?action=set' + '&sessionKey=' + sessionKey;
	
	loc += '&status=' + status;
	
	var code = 'location="' + loc + '"';
	
	eval(code);
}

